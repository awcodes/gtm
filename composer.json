{
    "name": "awcodes/gtm",
    "description": "Easy integration of Google Tag Manager into your Laravel application.",
    "keywords": [
        "awcodes",
        "laravel",
        "gtm",
        "google tag manager"
    ],
    "homepage": "https://github.com/awcodes/gtm",
    "license": "MIT",
    "authors": [
        {
            "name": "awcodes",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.17",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.3",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Awcodes\\Gtm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awcodes\\Gtm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\Gtm\\GtmServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
